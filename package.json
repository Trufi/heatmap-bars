{
    "name": "@trufi/heatmap",
    "version": "0.0.1",
    "description": "Heatmap bars for 2GIS MapGL API",
    "main": "dist/cjs/index.js",
    "module": "dist/module/index.js",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Trufi/heatmap.git"
    },
    "author": {
        "name": "Mstislav Zhivodkov",
        "email": "stevemyz@gmail.com",
        "url": "https://trufi.github.io/"
    },
    "license": "MIT",
    "scripts": {
        "start": "npm run dev",
        "build": "shx rm -rf dist && npm run build:module && npm run build:cjs",
        "build:module": "tsc",
        "build:cjs": "tsc -p tsconfig.cjs.json",
        "builddemo": "shx rm -rf demo/dist && webpack --env=production",
        "dev": "webpack serve",
        "prettier": "prettier --check \"./**/*.{ts,tsx,js,json,html,css}\"",
        "test": "npm run prettier && npm run build",
        "prepublishOnly": "shx rm -rf dist && npm run build"
    },
    "dependencies": {
        "2gl": "0.9.0",
        "@2gis/mapgl": "^1.22.0",
        "@trufi/utils": "0.0.10",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@types/dat.gui": "^0.7.7",
        "dat.gui": "^0.7.7",
        "fork-ts-checker-webpack-plugin": "^7.2.1",
        "prettier": "^2.5.1",
        "shx": "^0.3.4",
        "ts-loader": "^9.2.6",
        "typescript": "^4.5.5",
        "webpack": "^5.68.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4"
    }
}
