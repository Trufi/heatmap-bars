{
    "compilerOptions": {
        "target": "es6",
        "types": ["@2gis/mapgl/global", "@2gis/gl-matrix", "2gl"],
        "module": "es2015",
        "moduleResolution": "node",
        "lib": ["dom", "es6"],
        "sourceMap": true,
        "strict": true,
        "noEmitHelpers": true,
        "importHelpers": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "importsNotUsedAsValues": "preserve"
    }
}
